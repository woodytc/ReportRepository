<script type="text/javascript">
    Ext.Loader.setConfig({ enabled: true });
    Ext.Loader.setPath('Ext.ux', '@Url.Content("~/Scripts/extjs/ux")');
    Ext.require(['*']);
    Ext.QuickTips.init();
    
    //Report format {'function', controllername} 
    window.gridReportData = '@Url.Action("GridReport", "Home")';
    window.URLGridReport = '@Url.Action("GridReport", "Home")';
    window.URLCreateReport = '@Url.Action("CreateReport", "Home")';
    window.treedata = '@Url.Action("tree", "Home")';
    window.gridmappingasset = '@Url.Action("GridMappingAsset", "Home")';
    window.reportparam = '@Url.Action("ViewReport", "Report")';
    window.getParameterreport = '@Url.Action("GetParameterReport", "Home")';
    window.getAgrStatus = '@Url.Action("GetAgrStatus", "Home")';

    window.getMasterAssetType = '@Url.Action("GetMasterAssetType", "Home")';
    window.CreateMappingAsset = '@Url.Action("CreateMappingAsset", "Home")';
    window.DeleteMappingAsset = '@Url.Action("DeleteMappingAsset", "Home")';

    window.GridGetEQP = '@Url.Action("GridGetEQP", "Home")';
    window.CreateEQP = '@Url.Action("CreateEQP", "Home")';
    window.DeleteEQP = '@Url.Action("DeleteEQP", "Home")';
    window.UpdateEQP = '@Url.Action("UpdateEQP", "Home")';

    window.GridAssetType = '@Url.Action("GridAssetType", "Home")';
    window.CreateAssetType = '@Url.Action("CreateAssetType", "Home")';
    window.UpdateAssetType = '@Url.Action("UpdateAssetType", "Home")';
    window.DeleteAssetType = '@Url.Action("DeleteAssetType", "Home")';

    var currentUser = '@ViewBag.User';
    var actionLogoff = '@Url.Action("Logout", "User")';
    /*
    Register Tabs
    */
    var MappingAssetTypeTab = new MappingAssetTypeForm({ id: 'MappingAssetTypeForm', region: 'north' });
    var MappingCodeEQPTab = new EQPForm({ id: 'EQPForm', region: 'north' });
    var ListReportTab = new ListReportForm({ id: 'ListReportForm', region: 'north' });
    var ManageReportTab = new ManageReportFrom({ id: 'ManageReportFrom', region: 'north' });
    var AssetTypeTab = new AssetTypeForm({ id: 'AssetTypeForm', region: 'north' });
    
    /*
    *   
    */
    //add tabls
    var tabItems = [];
    tabItems.push(AssetTypeTab);
    tabItems.push(MappingCodeEQPTab);
    tabItems.push(MappingAssetTypeTab);
//    tabItems.push(ManageReportTab);
    tabItems.push(ListReportTab);
    

    //set Main Display
    var viewport = Ext.create('Ext.Viewport', {
        id: 'viewport',
        layout: 'border',
        //padding: 0,
        //width: '100%',
        border: true,
        items: [
                    {
                        xtype: 'panel',
                        region: 'north',
//                        layout: 'anchor',
//                        height: 25,
                        width:'100%',
                        title: 'KTBLEASING FL REPORT [ ' + currentUser + 
                                 ' ]<a href=' + actionLogoff + ' style="font-family:verdana;font-size:12px;color:red;text-decoration: none;">   Logoff</a>'
                    },
                    {
                        xtype: 'tabpanel',
                        region: 'center',
                        layout: 'anchor',
                        items: tabItems
                    }
            ]
    });

</script>

